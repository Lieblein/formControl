{
  "presets": [
    "stage-0",
    ["es2015", { "modules": false }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    }
  }
}

